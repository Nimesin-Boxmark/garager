#!/usr/bin/env ruby

require 'bundler/setup'
Bundler.require(:default)

REAL_GARAGE = !!(`uname -a` =~ /armv6l/)

require 'logger'

require 'garager/garage'
require 'garager/client'

Garager::Client.new(
  logger: Logger.new("log/client.log"),
  uri: "druby://underbee.linkleaf.com:8787",
  key: "533e98315f3de22fc4006674285aa837"
).listen
